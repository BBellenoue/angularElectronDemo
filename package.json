{
  "name": "angular-electron-demo",
  "version": "0.0.0",
  "main": "dist/angularElectronDemo/index.js",
  "build": {
    "appId": "com.sfeir.AngularElectronDemo",
    "productName": "AngularElectronDemo",
    "files": [
      "main.js",
      "dist/angularElectronDemo"
    ]
  },
  "scripts": {
    "ng": "ng serve",
    "clean": "rimraf dist",
    "electron": "wait-on http://localhost:4200 && electron electron-app.js",
    "prestart": "npm run clean",
    "start": "concurrently -k \"npm run compile-ts\" \"ng serve\" \"npm run electron\"",
    "prebuild": "npm run clean",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerelease": "npm run build -- --prod",
    "release": "electron-builder",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "compile-ts": "tsc electron-app.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.5.4",
    "sqlite3": "^4.1.1",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.24",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "electron-rebuild": "^1.10.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "wait-on": "^4.0.1"
  },
  "repository": {}
}
